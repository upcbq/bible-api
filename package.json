{
  "name": "quiz-bible-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "nodemon ./dist/index.js",
    "dev": "nodemon --exec ts-node -- -r tsconfig-paths/register ./src/index.ts --dir .",
    "prod": "npm run build && npm run start",
    "clean": "rm -rf node_modules coverage dist .nyc_output",
    "lint": "eslint 'src/**/*.ts' --fix",
    "ingestData": "ts-node -r tsconfig-paths/register src/scripts/ingestData.ts",
    "docker:up": "TAG=latest docker compose --env-file=./.env -f ./docker/docker-compose.yml up -d",
    "docker:down": "TAG=latest docker compose --env-file=./.env -f ./docker/docker-compose.yml down",
    "docker:build": "TAG=latest docker compose --env-file=./.env -f ./docker/docker-compose.yml build",
    "docker:push": "TAG=latest docker compose --env-file=./.env -f ./docker/docker-compose.yml push"
  },
  "keywords": [],
  "author": "Landon Bland",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status": "^1.7.3",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^4.0.0",
    "@types/lodash": "^4.14.202",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.3.3"
  }
}
